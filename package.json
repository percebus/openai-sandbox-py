{
  "name": "openai-sandbox-py",
  "version": "0.1.0",
  "description": "Azure OpenAI sandbox excercises",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "pip:upgrade": "python -m pip install --upgrade -v -r requirements.upgrade.txt",
    "npm:install:global:all": "npm install --location=global bower redis-cli open-cli",
    "npm:install:global:ci": "npm install --location=global bower",
    "setup": "npm run pip:upgrade && npm run pipx:install && npm run npm:install:global:all",
    "setup:ci": "npm run pip:upgrade && npm run pipx:install && npm run npm:install:global:ci",
    "pipx:install": "bash scripts/pipx/install.ba.sh",
    "python:isort": "python -m isort ./src",
    "python:black": "python -m black ./src",
    "python:pylint": "python -m pylint ./src",
    "prettier": "prettier .",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "lint": "npm run python:pylint && npm run prettier:check",
    "style": "npm run prettier:write && npm run python:isort && npm run python:black",
    "poststyle": "npm run lint",
    "test": "exit 0",
    "docker:compose:up": "docker compose up --build",
    "browser": "open-cli http://localhost:8001",
    "prestart": "npm run browser",
    "start": "npm run docker:compose:up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/percebus/openai-sandbox-py.git"
  },
  "keywords": [
    "openAI",
    "AI",
    "sandbox",
    "python",
    "azure"
  ],
  "author": "percebus",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/percebus/openai-sandbox-py/issues"
  },
  "homepage": "https://github.com/percebus/openai-sandbox-py#readme",
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
